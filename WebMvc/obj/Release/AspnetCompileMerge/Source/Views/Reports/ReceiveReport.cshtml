                                结束时间：<input type="text" name="endtime" id="endtime" value=" " class="mycalendar">

                                <input type="submit" value="查询" name="Save">
                                <input type="submit" value="导出" name="Import">
                                <div id="form_body_div" class="tabs_wrapper">
                                    <div class="tabs_main" align="left">
                                        <div id="PanelConfig">
                                            <div id="PanelDefault">

                                                <table class="table">
                                                    <thead>
                                                        <tr>


                                                            <td width="150">街道名称</td>
                                                            <td width="100">开始日期</td>
                                                            <td width="100">结束日期</td>
                                                            <td width="75">零报</td>
                                                            <td width="75">帮困</td>
                                                            <td width="75">减负</td>
                                                            <td width="75">交换次数</td>
                                                            

                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (var receivereports in Model)
                                                        {

                                                            <tr>

                                                                <td>@receivereports.Name</td>
                                                                <td>@receivereports.BeginDate</td>
                                                                <td>@receivereports.EndDate</td>
                                                                <td>@receivereports.Model</td>
                                                                <td>@receivereports.Unit</td>
                                                                <td>@receivereports.Type</td>
